{
  "name": "algobox-syntax-highlighting",
  "displayName": "Algobox syntax highlighting",
  "description": "Syntax support for algoBox algorithm language",
  "publisher": "ZayenJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZayenJS/algobox-syntax-highlighting.git"
  },
  "homepage": "https://github.com/ZayenJS/algobox-syntax-highlighting#readme",
  "bugs": {
    "url": "https://github.com/ZayenJS/algobox-syntax-highlighting/issues"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "algobox",
        "aliases": [
          "algoBox",
          "algobox",
          "algo"
        ],
        "extensions": [
          ".algo",
          ".algobox",
          ".box",
          ".ab"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "algobox",
        "scopeName": "source.algobox",
        "path": "./syntaxes/algobox.tmLanguage.json"
      }
    ]
  },
  "icon": "https://github.com/ZayenJS/algobox-syntax-highlighting/blob/main/img/algobox256x256.png"
}
